<center>
<% @cartedproducts.each do |cartedproduct| %>
<div>
<img src=<%=cartedproduct.product.images.first.image_1%>>
<p><%= cartedproduct.product.name%>
<%= cartedproduct.quantity%>
<%= button_to "Remove", "/cartedproducts/delete/#{cartedproduct.id}", class: "btn btn-danger btn-md"%></p>
</div>
<%end%>

<p><%= button_to "Buy", "/orders", class: "btn btn-success btn-lg"%></p>
<p><a class="btn btn-info btn-md" href="/products" role="button">Click Shop to Some More</a></p>
</center>
